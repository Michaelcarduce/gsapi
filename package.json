{
    "name": "gsapi",
    "version": "1.0.0",
    "description": "",
    "main": "src/server.js",
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "clean:all": "rimraf dist logs node_modules",
        "prebuild": "pnpm run clean",
        "build": "tsc && tsc-alias",
        "start": "tsx dist/src/server.js",
        "dev": "tsx --watch src/server.ts",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write \"src/**/*.{ts,json}\" \"**/*.ts\" \"prisma/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.{ts,json}\" \"**/*.ts\" \"prisma/**/*.ts\"",
        "type:check": "tsc --noEmit",
        "prisma:generate": "prisma generate",
        "prisma:migrate:dev": "prisma migrate dev",
        "prisma:seed": "prisma db seed",
        "test:unit": "jest",
        "test:unit:watch": "jest --watch",
        "postinstall": "prisma generate"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^6.11.1",
        "@prisma/extension-accelerate": "^2.0.2",
        "better-auth": "^1.2.12",
        "compression": "^1.8.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.0",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "morgan": "^1.10.0",
        "winston": "^3.17.0",
        "zod": "^4.0.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.0.12",
        "@types/supertest": "^6.0.3",
        "eslint": "^9.30.1",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.5.1",
        "jest": "^30.0.5",
        "prettier": "^3.6.2",
        "prisma": "^6.11.1",
        "rimraf": "^6.0.1",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.36.0"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} prisma/gen/seed.ts"
    }
}
